{
  "name": "vitepress-sidebar",
  "version": "1.7.5",
  "description": "A VitePress auto sidebar plugin that automatically creates a simple configuration.",
  "type": "module",
  "types": "dist/index.d.ts",
  "author": "jooy2 <jooy2.contact@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/jooy2/vitepress-sidebar",
  "repository": {
    "type": "git",
    "url": "https://github.com/jooy2/vitepress-sidebar"
  },
  "bugs": {
    "url": "https://github.com/jooy2/vitepress-sidebar/issues"
  },
  "scripts": {
    "build": "npm run format:fix && tsc --project tsconfig.prod.json && npm run minify",
    "test": "npm run build && mocha test/**/*.spec.ts -r ts-node/register --loader=ts-node/esm --timeout 10000",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "minify": "terser dist/index.js --config-file .terserrc -o dist/index.js",
    "prepare": "npm run build",
    "format": "prettier .",
    "format:fix": "prettier . --write"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "typesVersions": {
    "*": {
      "index.d.ts": [
        "dist/index.d.ts"
      ]
    }
  },
  "keywords": [
    "vitepress",
    "vitepress-plugin",
    "vitepress-sidebar",
    "vitepress-auto-sidebar",
    "vitepress-menu",
    "vitepress-sidemenu",
    "util",
    "utility",
    "sidebar",
    "sidebar-generator",
    "auto",
    "generator",
    "plugin",
    "sidemenu",
    "submenu",
    "menu",
    "autosidebar"
  ],
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "terser": "^5.17.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
