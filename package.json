{
  "name": "vitepress-sidebar",
  "version": "1.17.0",
  "description": "A VitePress auto sidebar plugin that automatically creates a simple configuration.",
  "type": "module",
  "types": "dist/index.d.ts",
  "author": "jooy2 <jooy2.contact@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/jooy2/vitepress-sidebar",
  "repository": {
    "type": "git",
    "url": "https://github.com/jooy2/vitepress-sidebar"
  },
  "bugs": {
    "url": "https://github.com/jooy2/vitepress-sidebar/issues"
  },
  "scripts": {
    "build": "npm run format:fix && tsc --project tsconfig.prod.json && npm run minify",
    "test": "npm run build && mocha test/**/*.spec.ts -r ts-node/register --loader=ts-node/esm --timeout 10000",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "minify": "terser dist/index.js --config-file .terserrc -o dist/index.js",
    "prepare": "npm run build",
    "format": "prettier .",
    "format:fix": "prettier . --write",
    "docs:dev": "npm run build && vitepress dev example",
    "docs:build": "npm run build && vitepress build example",
    "docs:serve": "npm run build && npm run docs:build && vitepress serve example"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "typesVersions": {
    "*": {
      "index.d.ts": [
        "dist/index.d.ts"
      ]
    }
  },
  "keywords": [
    "vitepress",
    "vitepress-plugin",
    "vitepress-plugin-sidebar",
    "vitepress-sidebar",
    "vitepress-auto-sidebar",
    "vitepress-plugin-auto-sidebar",
    "vitepress-menu",
    "vitepress-sidemenu",
    "util",
    "utility",
    "sidebar",
    "sidebar-generator",
    "generator",
    "plugin",
    "sidemenu",
    "submenu",
    "menu",
    "autosidebar"
  ],
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.6.3",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "mocha": "^10.2.0",
    "prettier": "^3.0.3",
    "terser": "^5.20.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vitepress": "^1.0.0-rc.15"
  },
  "dependencies": {
    "gray-matter": "^4.0.3"
  }
}
